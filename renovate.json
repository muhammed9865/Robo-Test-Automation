{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    "group:all"
  ],
  "packageRules": [
    {
      "matchPackagePatterns": [
        "org.jetbrains.kotlin.*"
      ],
        "groupName": "Kotlin"
    },
    {
      "matchPackagePatterns": [
        "androidx.compose.compiler:compiler"
      ],
        "groupName": "Kotlin"
    }
  ]
}
